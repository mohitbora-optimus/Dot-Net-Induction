

// for Loop

using System;

	class Loops
	{
		static void Print(int x)
		{
			for(int i=1; i<=10; i++)
			 {
			 	Console.WriteLine(i*x);
			 }
			
		}
	
		static void Main(string[] args)
		{
			
			int Number;
			Number = Console.Read();
			Print(Number);using System;

	class Loops
	{
		static void Print(int x)
		{ 
			int i= 1;
				do
				{
			 	 	Console.WriteLine(i*x);
			 	 	i++;
				} while(i<=10);
		}
	
		static void Main(string[] args)
		{
			
			int Number;
			Number = Console.Read();
			Print(Number);
		}
	}
	
	// while loop
	
	using System;

	class Loops
	{
		static void Print(int x)
		{ 
			int i= 1;
			while(i<=10)
				{
			 	 	Console.WriteLine(i*x);
			 	 	i++;
				} 
		}
	
		static void Main(string[] args)
		{
			
			int Number;
			Number = Console.Read();
			Print(Number);
		}
	}
		}
	}
	
	// do while loop
	
	
